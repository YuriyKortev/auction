doctype html
html
    head
        meta(charset="utf-8")
        link(rel="stylesheet" href="/stylesheets/users.css")
        link(rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css")
        title Участники
    body
        div(class="menu")
            h1 Выберите участника
            a(href='/' style='position:absolute;right:100px;') Назад
        hr
        hr


        div(class="admin")
            img(src="https://www.bsau.ru/upload/iblock/180/профиль%20мужской2.jpg")
            p admin

        div(id="list")
            -for (let value of users)
                div(id=value.id class='logged')
                    img(src="https://www.bsau.ru/upload/iblock/180/профиль%20мужской2.jpg")
                    p=value.name
                    p=value.surname
                    p(name="money")=value.money
                    p(name="id")='#'+value.id


        script(src="https://code.jquery.com/jquery-3.4.1.min.js")
        script(src="/javascripts/login.js")        